{
    "name": "genesyslite/genesys-catalog",
    "description": "Api para la parte de clientes",
    "type": "library",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mc Giver Avila Portocarrero",
            "email": "xmcgiver12@gmail.com"
        }
    ],
    "require": {
        "php" : "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "GenesysLite\\GenesysCatalog\\": "src/"
        }
    },
    "extra": {
